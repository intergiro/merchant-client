{
  "name": "@payfunc/merchant-client",
  "version": "0.0.18",
  "description": "Client for psp and acquirer",
  "author": "PayFunc",
  "license": "MIT",
  "repository": "https://github.com/payfunc/merchant-client",
  "bugs": {
    "url": "https://github.com/payfunc/merchant-client/issues"
  },
  "private": false,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "git": {
    "tagName": "v${version}"
  },
  "github": {
    "release": true
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.test.json"
      }
    },
    "transform": {
      "^.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(cloud-http|cryptly|authly|isoly|gracely|@payfunc)/.*)"
    ],
    "testEnvironment": "node",
    "testRegex": "((\\.|/)(test|spec))(\\.|\\/.+)(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "node_modules/",
      "dist/"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "preset": "ts-jest",
    "testMatch": null
  },
  "scripts": {
    "dev": "watch 'npm run build' .",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "fix": "eslint '*/**/*.{js,ts,tsx}' --fix",
    "build": "tsc -p .",
    "test": "jest --maxWorkers=2",
    "test:watch": "watch jest",
    "prepare": "npm run build",
    "clean": "rm -rf dist node_modules coverage"
  },
  "dependencies": {
    "@payfunc/model": "0.3.41",
    "@payfunc/model-acquirer": "0.1.110",
    "@payfunc/model-base": "0.1.31",
    "@payfunc/model-card": "0.2.34",
    "@payfunc/model-log": "0.1.20",
    "gracely": "0.1.7"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.0",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.6.0",
    "eslint-config-standardize": "^0.7.1",
    "eslint-plugin-prettierx": "github:nilssonemma/eslint-plugin-prettierx#master",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^26",
    "prettierx": "github:simonmika/prettierx#newline-before-clause",
    "ts-jest": "^26.1.0",
    "typescript": "^4.3.4",
    "watch": "^1.0.2"
  }
}
